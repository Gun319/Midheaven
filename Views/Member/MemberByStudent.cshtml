@model Midheaven.Models.Course
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>学生</title>
    <link href="~/script/bootstrap.min.css" rel="stylesheet" />
    <script src="~/script/jquery-3.4.1.min.js"></script>
    <script src="~/script/bootstrap.min.js"></script>
</head>
<body>
    <div>
        <nav class="navbar navbar-expand-lg navbar-light text-white-50 bg-secondary sticky-top">
            <a class="navbar-brand text-white-50" href="">中天教育</a>
            <span>@ViewBag.tName</span>
            <div class="collapse navbar-collapse" id="navbarSupportedContent">
                <ul class="navbar-nav mr-auto"></ul>
                <a href="/Member/SelStudentCourse" class="text-white-50 mr-3">查看已选的课程</a>
                <a href="/Member/MemberUpdateInfo" class="text-white-50 mr-3">更新个人信息</a>
                <form class="form-inline my-2 my-lg-0" method="post">
                    <label class="form-inline">
                        <span>课程名称：</span>@Html.TextBoxFor(m => m.C_Name, new { @class = "form-control", @placeholder = "搜索课程" })
                    </label>
                    <button type="button" id="Sel" class="btn btn-info ml-3 mr-5">查询</button>
                </form>
            </div>
        </nav>

        <div>
            <table class="table  table-striped text-center" id="dg">
                <thead class="thead-dark">
                    <tr>
                        <th>课程编号</th>
                        <th>课程名称</th>
                        <th>课程描述</th>
                        <th>课程图片</th>
                        <th>操作</th>
                    </tr>
                </thead>
                <tbody>
                </tbody>
            </table>
        </div>
    </div>
    <script src="~/script/MemberByStudent.js"></script>
</body>
</html>


